    "projectUrl": "http://imgurapi.readthedocs.org/en/latest",
    "licenseUrl": "https://github.com/DamienDennehy/Imgur.API/blob/master/LICENSE",
    "iconUrl": "https://raw.githubusercontent.com/DamienDennehy/Imgur.API/master/icon.png",
