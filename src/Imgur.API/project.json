{
  "version": "1.0.0-*",
  "authors": [ "Damien Dennehy" ],
  "description": "Imgur.API is a .NET implementation of Imgur's API. It supports Imgur's free and Mashape's commercial API endpoints.",
  "copyright": "Copyright Â© 2015-2017",
  "dependencies": {
    "Newtonsoft.Json": "9.0.1"
  },
  "frameworks": {
    "net45": {
      "frameworkAssemblies": {
        "System.Net.Http": "4.0.0.0"
      }
    },
    "netstandard1.1": {
      "dependencies": {
        "NETStandard.Library": "1.6.0"
      }
    }
  },
  "buildOptions": {
    "xmlDoc": true
  },
  "packOptions": {
    "tags": [ "imgur.api imgur.net imgur api mashape" ],
    "projectUrl": "http://imgurapi.readthedocs.org/en/latest",
    "licenseUrl": "https://github.com/DamienDennehy/Imgur.API/blob/master/LICENSE",
    "iconUrl": "https://raw.githubusercontent.com/DamienDennehy/Imgur.API/master/icon.png",
    "packInclude": {
      "readme.txt": "readme.txt"
    }
  }
}
